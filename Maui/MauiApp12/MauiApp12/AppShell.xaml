<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:MauiApp12.Pages"
    x:Class="MauiApp12.AppShell"
    Title="MauiApp12">

    <TabBar>

        <ShellContent
            Title="Input"
            ContentTemplate="{DataTemplate pages:InputPage}"
            Route="InputPage"/>

        <ShellContent
            Title="Reports"
            ContentTemplate="{DataTemplate pages:ReportPage}"
            Route="ReportPage"/>

    </TabBar>

</Shell>
